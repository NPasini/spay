# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'spay' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for spay
  pod 'Result'
  pod 'Swinject'
  pod 'SDWebImage'
  pod 'ReactiveSwift'
  pod 'ReactiveCocoa'

  target 'spayTests' do
    inherit! :search_paths
    # Pods for testing

    pod 'Quick'
    pod 'Nimble'

  end

  target 'spayUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

   
